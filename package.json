{
  "name": "genstuff",
  "description": "A new CLI generated with oclif",
  "version": "0.0.0",
  "author": "Alexey Sokolov",
  "bin": {
    "genstuff": "./bin/run.js"
  },
  "bugs": "https://github.com/@alexvyber/genstuff/issues",
  "dependencies": {
    "@inquirer/confirm": "^3.1.9",
    "@inquirer/input": "^2.1.9",
    "@inquirer/select": "^2.3.5",
    "@oclif/core": "^4",
    "@oclif/plugin-help": "^6",
    "@oclif/plugin-plugins": "^5",
    "chalk": "^5.3.0",
    "change-case": "^5.4.4",
    "ejs": "^3.1.10",
    "fs-extra": "^11.2.0"
  },
  "devDependencies": {
    "@oclif/prettier-config": "^0.2.1",
    "@oclif/test": "^4",
    "@types/chai": "^4",
    "@types/ejs": "^3.1.5",
    "@types/fs-extra": "^11.0.4",
    "@types/mocha": "^10",
    "@types/node": "^20.14.3",
    "chai": "^4",
    "eslint": "^9.5.0",
    "eslint-config-oclif": "^5",
    "eslint-config-oclif-typescript": "^3",
    "eslint-config-prettier": "^9",
    "mocha": "^10",
    "oclif": "^4.13.8",
    "shx": "^0.3.3",
    "ts-node": "^10",
    "typescript": "^5"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "files": ["/bin", "/dist", "/oclif.manifest.json"],
  "homepage": "https://github.com/@alexvyber/genstuff",
  "keywords": ["oclif"],
  "license": "MIT",
  "main": "dist/index.js",
  "type": "module",
  "oclif": {
    "bin": "genstuff",
    "dirname": "genstuff",
    "commands": "./dist/commands",
    "plugins": ["@oclif/plugin-help", "@oclif/plugin-plugins"],
    "topicSeparator": " ",
    "topics": {
      "hello": {
        "description": "Say hello to the world and others"
      }
    }
  },
  "repository": "@alexvyber/genstuff",
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "lint": "eslint . --ext .ts",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "pnpm run lint",
    "prepack": "oclif manifest && oclif readme",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif readme && git add README.md"
  },
  "types": "dist/index.d.ts",
  "packageManager": "pnpm@8.15.1+sha1.8adba2d20330c02d3856e18c4eb3819d1d3ca6aa"
}
