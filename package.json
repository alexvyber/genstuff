{
  "name": "genstuff",
  "version": "0.0.0-alpha.6",
  "description": "Generate best React components ever",
  "keywords": [
    "oclif"
  ],
  "homepage": "https://github.com/alexvyber/genstuff",
  "bugs": "https://github.com/alexvyber/genstuff/issues",
  "repository": "alexvyber/genstuff",
  "license": "MIT",
  "author": "Alexey Sokolov @alexvyber",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "genstuff": "./bin/run"
  },
  "files": [
    "/bin",
    "/dist",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "clean": "shx rm -rf src/components && mkdir src/components",
    "lint": "eslint . --ext .ts --config .eslintrc.js",
    "prepack": "yarn build && oclif manifest && oclif readme",
    "postpack": "shx rm -f oclif.manifest.json",
    "version": "oclif readme && git add README.md",
    "publish": "yarn prepack && npm publish && yarn postpack"
  },
  "dependencies": {
    "@oclif/core": "^2",
    "@oclif/plugin-help": "^5",
    "@oclif/plugin-plugins": "^2.3.2"
  },
  "devDependencies": {
    "@oclif/test": "^2.3.6",
    "@types/chai": "^4",
    "@types/node": "^16.18.12",
    "oclif": "^3.7.0",
    "shx": "^0.3.3",
    "ts-node": "^10.9.1",
    "tslib": "^2.5.0",
    "typescript": "^4.9.5"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "oclif": {
    "bin": "genstuff",
    "dirname": "genstuff",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-plugins"
    ],
    "topicSeparator": " ",
    "topics": {
      "hello": {
        "description": "Say hello to the world and others"
      }
    }
  }
}
