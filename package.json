{
  "name": "genstuff",
  "version": "0.0.1-dev.1",
  "description": "A new CLI generated with oclif",
  "keywords": [
    "oclif"
  ],
  "homepage": "https://github.com/repos/genstuff",
  "bugs": "https://github.com/repos/genstuff/issues",
  "repository": "repos/genstuff",
  "license": "MIT",
  "author": "Alexey Sokolov",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": "./dist/index.js",
    "./actions": "./dist/actions/index.js"
  },
  "bin": {
    "gen": "./bin/run.js"
  },
  "files": [
    "./bin",
    "./dist",
    "./oclif.manifest.json"
  ],
  "scripts": {
    "build": "shx rm -rf dist && tsc -b --noCheck",
    "lint": "eslint",
    "prepack": "oclif manifest && oclif readme",
    "postpack": "shx rm -f oclif.manifest.json",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "posttest": "pnpm run lint",
    "version": "oclif readme && git add README.md"
  },
  "oclif": {
    "bin": "gen",
    "commands": "./dist/commands",
    "dirname": "gen",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-plugins"
    ],
    "topicSeparator": " ",
    "topics": {
      "hello": {
        "description": "Say hello to the world and others"
      }
    }
  },
  "dependencies": {
    "@oclif/core": "^4",
    "@oclif/plugin-help": "^6",
    "@oclif/plugin-plugins": "^5",
    "@sinclair/typebox": "^0.34.33",
    "@types/bun": "^1.2.9",
    "c12": "^3.2.0",
    "chalk": "^5.4.1",
    "change-case": "^5.4.4",
    "del": "^8.0.0",
    "enquirer": "^2.4.1",
    "es-toolkit": "^1.34.1",
    "globby": "^14.1.0",
    "handlebars": "^4.7.8",
    "interpret": "^3.1.1",
    "isbinaryfile": "^5.0.4",
    "liftoff": "^5.0.0",
    "lilconfig": "^3.1.3",
    "minimist": "^1.2.8",
    "mkdirp": "^3.0.1",
    "ora": "^8.2.0",
    "smol-toml": "^1.3.1",
    "title-case": "^4.3.2",
    "v8flags": "^4.0.1",
    "valibot": "1.1.0"
  },
  "devDependencies": {
    "@eslint/compat": "^1",
    "@oclif/prettier-config": "^0.2.1",
    "@oclif/test": "^4",
    "@types/chai": "^5.2.2",
    "@types/mocha": "^10",
    "@types/node": "^24.3.0",
    "chai": "^6.0.1",
    "eslint": "^9",
    "eslint-config-oclif": "^6",
    "eslint-config-prettier": "^10",
    "mocha": "^11.7.1",
    "oclif": "^4",
    "shx": "^0.4.0",
    "ts-node": "^10",
    "typescript": "^5"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
